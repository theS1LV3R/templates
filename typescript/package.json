{
  "name": "typescript-template",
  "version": "0.1.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "author": "S1LV3R <s1lv3r@corax.team>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "start:dev": "cross-env NODE_ENV=development nodemon src/index.ts",
    "start:debug": "cross-env NODE_ENV=development nodemon --exec node --trace-warnings --inspect --require ts-node/register src/index.ts",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "prebuild": "rimraf dist",
    "build": "tsc",
    "version": "standard-version",
    "postversion": "echo \"Remember to sign any tags!\"",
    "commit": "commit",
    "prepare": "husky install"
  },
  "dependencies": {},
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@commitlint/prompt-cli": "^16.2.1",
    "@commitlint/types": "^16.2.1",
    "@types/node": "^17.0.17",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.5.5"
  }
}
